{
	"PINYIN_MAP": {
		{% set comma = joiner(",") %}
		{% for cprovince in registry.keys() %}
		{% for city in registry[cprovince] %}
		{{ comma() }} "{{city[0]}}": "{{city[1]}}"
		{% endfor %}
		{% endfor %}
	},
	"FILE_MAP": {
		{% set comma = joiner(",") %}
		{% for cprovince in registry.keys() %}
		{% for city in registry[cprovince] %}
		{{ comma() }} "{{city[1]}}": "{{names[cprovince]}}/{{city[1]}}.min.js"
		{% endfor %}
		{% endfor %}
	}
}
